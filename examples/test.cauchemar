PROGRAM:
  EXAMPLE-STACK-MANIPULATORS
  EXAMPLE-EQUALITY
  EXAMPLE-BOOLEAN-OPERATIONS
  EXAMPLE-ARITHMETIC
  EXAMPLE-CONDITIONAL
  EXAMPLE-LOOP
  EXAMPLE-COMPARISONS
  EXAMPLE-TYPE-CHECK
  EXAMPLE-TO
  "ALL TESTS PASSED" PRINT
  IS-EMPTY ASSERT

EXAMPLE-STACK-MANIPULATORS:
  IS-EMPTY ASSERT
  0 
  IS-EMPTY NOT ASSERT
  COUNT 1 EQUALS ASSERT
  DROP
  IS-EMPTY ASSERT
  COUNT 0 EQUALS ASSERT
  "DROP PASS" PRINT

  0
  COUNT 1 EQUALS ASSERT
  DUP
  COUNT 2 EQUALS ASSERT
  DROP DROP
  IS-EMPTY ASSERT
  "DUP PASS" PRINT

  0 1
  COUNT 2 EQUALS ASSERT
  DUP   1 EQUALS ASSERT
  SWAP
  COUNT 2 EQUALS ASSERT
  DUP   0 EQUALS ASSERT
  DROP DROP
  IS-EMPTY ASSERT
  "SWAP PASS" PRINT

  0 1 2
  COUNT 3 EQUALS ASSERT
  DUP   2 EQUALS ASSERT
  ROT ; => 1 2 0
  COUNT 3 EQUALS ASSERT
  DUP   0 EQUALS ASSERT
  ROT ; => 2 0 1
  COUNT 3 EQUALS ASSERT
  DUP   1 EQUALS ASSERT
  ROT ; => 0 1 2
  COUNT 3 EQUALS ASSERT
  DUP   2 EQUALS ASSERT
  DROP DROP DROP
  IS-EMPTY ASSERT
  "ROT PASS" PRINT

  0 1
  COUNT 2 EQUALS ASSERT
  DUP   1 EQUALS ASSERT
  OVER ; => 0 1 0
  COUNT 3 EQUALS ASSERT
  DUP   0 EQUALS ASSERT
  OVER ; => 0 1 0 1
  COUNT 4 EQUALS ASSERT
  DUP   1 EQUALS ASSERT
  DROP DROP DROP DROP
  IS-EMPTY ASSERT
  "OVER PASS" PRINT


EXAMPLE-EQUALITY:
  1    1     EQUALS     ASSERT
  1    2     EQUALS NOT ASSERT
  "INTEGER EQUALITY PASS" PRINT

  TRUE TRUE  EQUALS     ASSERT
  TRUE FALSE EQUALS NOT ASSERT
  "BOOLEAN EQUALITY PASS" PRINT

  "A"  "A"   EQUALS     ASSERT
  "A"  "B"   EQUALS NOT ASSERT
  "STRING EQUALITY PASS"  PRINT

EXAMPLE-BOOLEAN-OPERATIONS:
  TRUE  TRUE  AND TRUE  EQUALS ASSERT
  TRUE  FALSE AND FALSE EQUALS ASSERT
  FALSE TRUE  AND FALSE EQUALS ASSERT
  FALSE FALSE AND FALSE EQUALS ASSERT
  "AND PASS" PRINT

  TRUE  TRUE  OR TRUE  EQUALS ASSERT
  TRUE  FALSE OR TRUE  EQUALS ASSERT
  FALSE TRUE  OR TRUE  EQUALS ASSERT
  FALSE FALSE OR FALSE EQUALS ASSERT
  "OR PASS"  PRINT

  TRUE  NOT FALSE EQUALS ASSERT
  FALSE NOT TRUE  EQUALS ASSERT
  "NOT PASS" PRINT

EXAMPLE-ARITHMETIC:
  1 2 +  3 EQUALS ASSERT
  1 2 - -1 EQUALS ASSERT
  1 2 *  2 EQUALS ASSERT
  1 2 /  0 EQUALS ASSERT
  "MATH ARITHMETIC PASS" PRINT

EXAMPLE-CONDITIONAL:
  TRUE IF TRUE ASSERT ELSE FALSE ASSERT THEN
  FALSE IF FALSE ASSERT THEN
  "CONTROL FLOW PASS" PRINT

EXAMPLE-LOOP:
  5 DO
    1 -
    DUP 0 GREATER-THAN
  WHILE
  0 EQUALS ASSERT
  "LOOP PASS" PRINT

EXAMPLE-COMPARISONS:
  1 2 GREATER-THAN FALSE EQUALS ASSERT
  2 1 GREATER-THAN TRUE  EQUALS ASSERT
  1 1 GREATER-THAN FALSE EQUALS ASSERT
  "GREATER-THAN PASS" PRINT

  1 2 GREATER-EQUAL FALSE EQUALS ASSERT
  2 1 GREATER-EQUAL TRUE  EQUALS ASSERT
  1 1 GREATER-EQUAL TRUE  EQUALS ASSERT
  "LESS-THAN PASS" PRINT

  1 2 LESS-THAN TRUE  EQUALS ASSERT
  2 1 LESS-THAN FALSE EQUALS ASSERT
  1 1 LESS-THAN FALSE EQUALS ASSERT
  "LESS-THAN PASS" PRINT

  1 2 LESS-EQUAL TRUE  EQUALS ASSERT
  2 1 LESS-EQUAL FALSE EQUALS ASSERT
  1 1 LESS-EQUAL TRUE  EQUALS ASSERT
  "LESS-EQUAL PASS" PRINT

EXAMPLE-TYPE-CHECK:
  TRUE IS-BOOLEAN     ASSERT DROP
  0    IS-BOOLEAN NOT ASSERT DROP
  "A"  IS-BOOLEAN NOT ASSERT DROP
  "IS-BOOLEAN PASS" PRINT

  TRUE IS-NUMBER NOT ASSERT DROP
  0    IS-NUMBER     ASSERT DROP
  "A"  IS-NUMBER NOT ASSERT DROP
  "IS-NUMBER PASS" PRINT

  TRUE IS-STRING NOT ASSERT DROP
  0    IS-STRING NOT ASSERT DROP
  "A"  IS-STRING     ASSERT DROP
  "IS-STRING PASS" PRINT

EXAMPLE-TO:
  0 5 TO
  COUNT 6 EQUALS ASSERT
  5 EQUALS ASSERT
  4 EQUALS ASSERT
  3 EQUALS ASSERT
  2 EQUALS ASSERT
  1 EQUALS ASSERT
  0 EQUALS ASSERT
  "TO DESCENDING PASS" PRINT

  5 0 TO
  COUNT 6 EQUALS ASSERT
  0 EQUALS ASSERT
  1 EQUALS ASSERT
  2 EQUALS ASSERT
  3 EQUALS ASSERT
  4 EQUALS ASSERT
  5 EQUALS ASSERT
  "TO ASCENDING PASS" PRINT